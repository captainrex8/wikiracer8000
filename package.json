{
  "name": "wikiracer8000",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/captainrex8/wikiracer8000.git",
  "author": "Rex Chen <rex@panoridge.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": false,
  "scripts": {
    "start": "node index.js",
    "lint": "eslint .",
    "test": "yarn lint && NODE_ENV=test jest --coverage"
  },
  "jest": {
    "verbose": true,
    "resetMocks": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/config/"
    ],
    "coveragePathIgnorePatterns": [
      "tests",
      "node_modules",
      "config"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 40,
        "branches": 40,
        "functions": 40,
        "lines": 40
      }
    }
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "jest": "^26.0.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  }
}
